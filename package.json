{
  "name": "gapp",
  "private": true,
  "version": "0.0.0",
  "module": "esnext",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "build": "tsc && npx prisma generate && vite build",
    "test": "jest",
    "preview": "vite preview",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-graphql": "graphql-code-generator --config graphql/codegen.ts"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/is-land": "^3.0.0",
    "@apollo/server": "^4.3.0",
    "@graphql-tools/utils": "^9.1.3",
    "@prisma/client": "^4.7.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@splinetool/runtime": "^0.9.148",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/express-graphql": "^0.9.0",
    "@types/glob": "^8.0.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.2.3",
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "glob": "^8.0.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "graphql-tools": "^8.3.14",
    "jest": "^29.3.1",
    "msw": "^0.49.1",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "prisma": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/client-preset": "^1.2.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^8.3.0",
    "jest-environment-jsdom": "^29.3.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
